DIM A, B, C, D, I

A = 1
B = 2
C = "HELLO"
D = "WORLD"

PRINT "A: "; A
PRINT "B: "; B
PRINT "C: "; C
PRINT "D: "; D
PRINT " "

REM Print a separator
I = 1
LOOP WHILE I < 9
	LOCATE I, 10
	PRINT "|"
	I = I + 1
END LOOP

IF A == B THEN
	PRINT "A == B"
END IF

LOCATE 1, 12

IF A <> B THEN
	PRINT "A <> B"
END IF

LOCATE 2, 12

IF A > B THEN
	PRINT "A > B"
END IF

LOCATE 3, 12

IF A < B THEN
	PRINT "A < B"
END IF

LOCATE 4, 12

IF A >= B THEN
	PRINT "A >= B"
END IF

LOCATE 5, 12

IF A <= B THEN
	PRINT "A <= B"
END IF

LOCATE 6, 12

IF C == D THEN
	PRINT "C == D"
END IF

LOCATE 7, 12

IF C == "HELLO" THEN
	PRINT "C == 'HELLO'"
END IF

LOCATE 8, 12

IF D == "HELLO" THEN
	PRINT "D == 'HELLO'"
END IF

END
